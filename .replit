modules = ["nodejs-20", "web"]
run = "npm run dev"
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist", "tests"]

[nix]
channel = "stable-24_05"
packages = ["imagemagick_light", "nedit", "supabase-cli"]

[deployment]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]
run = ["npm", "run", "start"]

[env]
PORT = "5000"
NODE_ENV = "development"

[agent]

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5001
externalPort = 3000

[[ports]]
localPort = 5002
externalPort = 8080

[[ports]]
localPort = 5003
externalPort = 3001

[objectStorage]
defaultBucketID = "replit-objstore-80f613b7-e782-4754-9b10-4f5bb52443a8"
